<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:rbd="http://www.sat.gob.gt/face2/ComplementoReferenciaBoletaDespacho/0.1.0"
	targetNamespace="http://www.sat.gob.gt/face2/ComplementoReferenciaBoletaDespacho/0.1.0"
	elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="ReferenciaBoletaDespacho">
